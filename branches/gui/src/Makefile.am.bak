## Process this file with automake to produce Makefile.in

## Created by Anjuta

AM_CPPFLAGS = \
	-DPACKAGE_LOCALE_DIR=\""$(prefix)/$(DATADIRNAME)/locale"\" \
	-DPACKAGE_SRC_DIR=\""$(srcdir)"\" \
	-DPACKAGE_DATA_DIR=\""$(datadir)"\" \
	$(SUECA_CFLAGS)

AM_CFLAGS =\
	 -Wall\
	 -g

bin_PROGRAMS = sueca

sueca_SOURCES = \
	main.c \
	cards.c \
	cards.h \
	deck.c \
	deck.h \
	player.c \
	player.h \
	hand.c \
	hand.h \
	table.c \
	table.h \
	team.c \
	team.h \
	trick.c

sueca_LDFLAGS = \
	-Wl,--export-dynamic

sueca_LDADD = $(SUECA_LIBS)

EXTRA_DIST = $(glade_DATA)
